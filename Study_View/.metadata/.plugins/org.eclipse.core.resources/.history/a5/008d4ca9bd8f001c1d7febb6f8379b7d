$(document).ready(function() {
	
	var id = document.getElementById("id");
	var pw = document.getElementById("pw");

	$("#loginBtn").click(function() {
		
		if (!$("#id").val()) {
			alert("아이디를 입력하세요");
			$("#id").focus();
			return false;
		}
		
		if (!$("#pw").val()) {
			alert("비밀번호를 입력하세요");
			$("#pw").focus();
			return false;
		}
		
		
		
		
		
	}); //loginBtn click
	
	
	
	
	
	
	
	
	
}); //ready




























